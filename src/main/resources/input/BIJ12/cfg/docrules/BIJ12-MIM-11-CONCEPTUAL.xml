<doc-rules type="config" xmlns:xi="http://www.w3.org/2001/XInclude">
    
    <xi:include href="../../../Geonovum/cfg/docrules/Geonovum-MIM-11-CONCEPTUAL.xml"/>
    
    <name>BIJ12-MIM-1-CONCEPTUAL</name>
    
    <desc>BIJ12 SIM specific requirements on documentation. Follows Geonovum.</desc>
    
    <!-- 
        collect the codelist and enumeration contents in a separate section?
    -->
    <lists-to-listing>false</lists-to-listing>
    
    <!-- 
         Moeten de details van attribuutsoorten uit een gegevensgroeptype alleen worden opgenomen in de sectie voor gegevengroeptypen ('group'), of ook bij de objecten ('object')?
         Default is: 'object'. 
     -->
    <gegevensgroep-attribute-container>group</gegevensgroep-attribute-container>
    
    <!--
        Wanneer ReSpec wordt samengesteld, welke config kan worden gebruikt? 
    -->
    <respec-config>https://gitdocumentatie.logius.nl/publicatie/respec/builds/respec-nlgov.js</respec-config>
    
</doc-rules>