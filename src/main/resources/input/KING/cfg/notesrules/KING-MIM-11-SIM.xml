<notes-rules type="config" xmlns:xi="http://www.w3.org/2001/XInclude">
  
    <name>KING-MIM-11-SIM</name>
    
    <desc>KING only allows definitions to occur in notes field.</desc>
    
    <notes-format>plain</notes-format><!-- plain, markdown, mediawiki -->
    
    <notes-rule lang="nl" default="Definitie">
        <section title="Definitie" tagged-value="CFG-TV-DEFINITION">
            <!-- no labels -->
        </section>
        <section title="Toelichting" tagged-value="CFG-TV-DESCRIPTION">
            <!-- no labels -->
        </section>
        <section title="Patroon" tagged-value="CFG-TV-PATTERN">
            <!-- no labels -->
        </section>
    </notes-rule>
    
</notes-rules>