<config>
   <id>
      <name>Tester parameters</name>
      <version-control>$Id: parms.xml 7431 2016-02-24 12:46:42Z arjan $</version-control>
   </id>
   <cli-parms>
      <cli-parm>
         <name>infile</name>
         <arg>filepath</arg>
         <tip>Input file for generic transformation</tip>
         <required>true</required>
         <type>file</type>
      </cli-parm> 
      <cli-parm>
         <name>outfile</name>
         <arg>filepath</arg>
         <tip>Output file for generic transformation</tip>
         <required>true</required>
         <type>file</type>
      </cli-parm> 
      <cli-parm>
         <name>xslfile</name>
         <arg>filepath</arg>
         <tip>XSL file for generic transformation</tip>
         <required>true</required>
         <type>file</type>
      </cli-parm> 
      <cli-parm>
         <name>extension</name>
         <arg>string</arg>
         <tip>Extension, to append to result file name (e.g. ".html")</tip>
         <type>string</type>
         <default>.html</default>
      </cli-parm> 
      <cli-parm>
         <name>transinfo</name>
         <arg>string</arg>
         <tip>Information to pass to the transformation stylesheet</tip>
         <type>string</type>
         <default><!--geen--></default>
      </cli-parm> 
   </cli-parms>
   
   <properties>

   </properties>
</config>
