<config>
   <id>
      <name>JSON schema compiler</name>
      <version-control>$Id: parms.xml 7509 2016-04-25 13:30:29Z arjan $</version-control>
   </id>
   
   <cli-parms>
      <cli-parm>
         <name>createjsonconcepts</name>
         <arg>yes|no</arg>
         <tip>Yes if JSON concepts must be generated</tip>
         <default>no</default>
      </cli-parm> 
      <cli-parm>
         <name>jsonconceptsname</name>
         <arg>string</arg>
         <tip>The name of the JSON concepts file generated</tip>
         <default>no_jsonconceptsname</default>
      </cli-parm> 
   </cli-parms>
   
   <properties>
   
      <WORK_JSONCONCEPTS_XMLPATH>${system/work-imvert-folder-path}/imvertor.27.jsonconcepts.xml</WORK_JSONCONCEPTS_XMLPATH>   
      <WORK_JSONCONCEPTS_JSONPATH>${system/work-imvert-folder-path}/imvertor.28.jsonconcepts.json</WORK_JSONCONCEPTS_JSONPATH>   
     
      <IMVERTOR_JSONCONCEPTS_XSLPATH>JsonConceptsCompiler.xsl</IMVERTOR_JSONCONCEPTS_XSLPATH>
      
   </properties>
   
</config>
