<config>
   <id>
      <name>Readme compiler</name>
      <version-control>$Id: parms.xml 7431 2016-02-24 12:46:42Z arjan $</version-control>
   </id>
   <cli-parms>
      <cli-parm>
         <name>contactemail</name>
         <arg>email-address</arg>
         <tip>The email address for support on the (form of the) distributed release</tip>
         <required>false</required>
      </cli-parm> 
      <cli-parm>
         <name>contacturl</name>
         <arg>url</arg>
         <tip>The URL for support on the (form of the) distributed release</tip>
         <required>false</required>
      </cli-parm> 
   </cli-parms>
   
   <properties>

      <IMVERTOR_README_XSLPATH>Imvert2readme.xsl</IMVERTOR_README_XSLPATH>
      
   </properties>
</config>
